{
    "name": "useless-api",
    "projectType": "application",
    "root": "apps/useless-api",
    "sourceRoot": "apps/useless-api/src",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./gradlew build",
          "cwd": "apps/useless-api"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./gradlew bootRun",
          "cwd": "apps/useless-api"
        }
      },
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./gradlew clean",
          "cwd": "apps/useless-api"
        }
      }
    },
    "tags": []
  }
  